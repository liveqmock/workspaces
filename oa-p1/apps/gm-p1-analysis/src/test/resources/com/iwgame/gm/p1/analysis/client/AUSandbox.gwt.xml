<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='AUSandbox'>
	<!-- Inherit the AU  -->
	
	<inherits name='com.iwgame.gm.p1.analysis.P1Analysis' />
	<inherits name="com.iwgame.xportal.report.Report" />
	<inherits name="com.iwgame.xportal.common.Common" />
	
	<inherits name='com.iwgame.xmvp.XMVP'	 />
	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.standard.Standard'/>
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->
	<!-- Specify the app entry point class. -->
	
	
	
	<entry-point class='com.iwgame.gm.p1.analysis.client.AUSandbox' />


	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

	
	<set-property name="user.agent" value="gecko1_8,safari"/>
</module>
