<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">

<dwr>
	<allow>
		<create javascript="RemoteService" creator="spring">
			<param name="beanName" value="dwrService" />
		</create>

		<convert converter="exception" match="java.lang.Exception" />
		<convert converter="bean" match="com.iwgame.csp.support.Message" />
		<convert converter="bean" match="com.iwgame.csp.support.exception.ServiceException" />
		<convert converter="bean" match="java.lang.StackTraceElement" />
		
	</allow>
	<signatures>
		<![CDATA[
			import java.util.HashMap;
		]]>
	</signatures>
</dwr>